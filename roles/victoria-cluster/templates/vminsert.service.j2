[Unit]
Description=Victoria Metrics vminsert service
After=network.target

[Service]
User={{victoria_user}}
Group={{victoria_user}}
WorkingDirectory={{victoria_path}}
ExecStart={{victoria_path}}/vminsert-prod -storageNode=192.168.1.10,192.168.1.11
Restart=always
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target